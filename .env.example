# 环境变量配置示例
# 复制此文件为 .env 并填入你的API密钥

# 通用API密钥（可选，会被其他API密钥覆盖）
LLM_API_KEY=your_generic_api_key_here

# 豆包API配置（默认使用）
DOBAO_API_KEY=your_doubao_api_key_here
DOBAO_API_SECRET=your_doubao_api_secret_here

# OpenAI API配置
OPENAI_API_KEY=your_openai_api_key_here

# DeepSeek API配置（可选）
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Moonshot API配置（可选）
MOONSHOT_API_KEY=your_moonshot_api_key_here

# Qwen API配置（可选）
QWEN_API_KEY=your_qwen_api_key_here

# 日志级别
LOG_LEVEL=INFO

# 数据目录
DATA_DIR=./data
OUTPUT_DIR=./output